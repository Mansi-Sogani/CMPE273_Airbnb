<body ng-app="airbnb" ng-controller="dashboardController">
	<div style="z-index: 99; display: block;">
		<nav class="navbar navbar-default "
			style="background-color: transparent; background: transparent; border-color: transparent;">
			<div class="container-fluid">
				<div class="navbar-header">
					<a class="navbar-brand" href="/home"
						style="border-right: 1px solid #dce0e0;"> <img
						src="https://a1.muscache.com/airbnb/static/icons/android-icon-192x192-c0465f9f0380893768972a31a614b670.png"
						alt="some_text"
						style="height: 30px; font-weight: normal; font-size: inherit; background-color: transparent; color: inherit; top: -2px; font-family:"HelveticaNeue";font-size: 14px;    padding: 4px 12px;     ">
					</a>

				</div>
				<ul class="nav navbar-nav pull-right" style="font-family: Circular,"HelveticaNeue",Helvetica; top: -2px;font-size: 14px;padding: 4px 12px;"-webkit-font-smoothing:antialiased;">
					
					<li><a href="/getAdminDashboard"> <img width="28" height="28" class="img-circle" ng-src="{{imageUrl}}" style="border-color: #c4c4c4; border: 2px solid #c4c4c4;">
					</a></li>
					<li>
				</ul>
			</div>
		</nav>
	</div>
	<div>
	</div>
	<div class="row">
		<div class="col-md-1"></div>
		<div class="col-md-10">
			<div class="tabbable">
				<ul class="nav nav-pills nav-stacked col-md-3">
					<li class="active"><a href="#a" data-toggle="tab">View Profile</a></li>
					<li><a href="#b" data-toggle="tab">View Properties Listed</a></li>
					<li><a href="#c" data-toggle="tab">View Reviews</a></li>
					<li><a href="#d" data-toggle="tab">Add Reviews</a></li>
				</ul>
				<div class="tab-content col-md-9">
					<div class="tab-pane active" id="a">
						<div class="panel panel-default">
							<div class="panel-heading" align="middle" style="font-size: auto" ng-model="product_name_model">
								<strong><h3>Host</h3></strong>
							</div>
							<div class="panel-body">
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Name</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"><span><%=host[0].fname%> <%=host[0].lname%></span> </span>
									</div>
								</div>
								<div class="row" style="height: 20px"></div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Total Listings</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"></span>
									</div>
								</div>
								<div class="row" style="height: 20px"></div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Phone
											number</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"><%=host[0].phone_no%></span>
									</div>
								</div>

								<div class="row" style="height: 20px"></div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Birthday</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"><%=host[0].birthday%></span>
									</div>
								</div>

								<div class="row" style="height: 20px"></div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Address</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;">
											<%=host[0].street%>, <%=host[0].apartment%>, <%=host[0].city%>,
											<%=host[0].state%>, <%=host[0].zip_code%> </span>
									</div>
								</div>
							</div>

							<div class="panel-footer"></div>
						</div>
					</div>
					
					
					
					
					
					
					
					<div class="tab-pane" id="b">
						<div class="panel panel-default">
							<div class="panel-heading" align="middle" style="font-size: auto"
								ng-model="product_name_model">
								<strong><h3>View Properties</h3></strong>
							</div>
							
							<% if(host.length){ 
					          for(var i = 0;i < host.length;i++) { %> 
							<div class="panel-footer"style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
							     <h3><b><%=(i+1)%></b></h3>   
							</div>
							
							<div class="panel-body" style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
							<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Title</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"><span><%=host[i].title%></span> </span>
									</div>
								</div>
							</div>
							<div class="panel-body" style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
							<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Description</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"><span><%=host[i].desc%></span> </span>
									</div>
								</div>
							</div>
								<div class="panel-body" style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Guests</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"><span><%=host[i].guests%></span> </span>
									</div>
								</div>
								</div>
								
								<div class="panel-body" style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Rooms</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"><span><%=host[i].rooms%></span> </span>
									</div>
								</div>
								</div>
							
								<div class="panel-body" style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Address</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"><%=host[i].street%>, <%=host[i].city%>, <%=host[i].state%>, <%=host[i].zip%></span> </span>
									</div>
								</div>
								</div>
							
								<div class="panel-body" style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11">
										<span style="font-size: 18px; font-weight: bold; font-family: Circular,"HelveticaNeue",Helvetica; ">Bidding Allowed</span>
									</div>
								</div>
								<div class="row">
									<div class="col-md-1"></div>
									<div class="col-md-11"
										style="font-size: 14px; font-family: Circular,"HelveticaNeue",Helvetica; ">
										<div class="row" style="height: 10px"></div>
										<span style="font-weight: bold;"><span><%=host[i].IsBiddable=0 ? 'Yes' : 'No' %></span> </span>
									</div>
								</div>
							</div>
							<hr>
							 <% }            
				            }else{ %>
										                 
				            <% } %> 
										            
						</div>
					</div>
					<div class="tab-pane" id="c">
						<div class="panel panel-default">
							<div class="panel-heading" align="middle" style="font-size: auto"ng-model="product_name_model">
								<strong><h3>View Reviews</h3></strong>
							</div>
							<div class="panel-footer">
								<% if(host.length){ 
								    for(var i = 0; i<host.length ; i++) { %>
							<div class="row"><br>
							<div class="col-sm-10" >
							<table class="table table-bordered table-striped">
								<thead>
									<tr class>
									<th>Reviews</th>
									<th width="12%">Ratings</th>	
									</tr>
								</thead>
								<tbody>
									<td><%=host[i].review%></td>
									<td><%=host[i].rating%></td>
									</tr>
								</tbody>
							</table>
							</div>
							</div>
							<% }            
										            }else{ %>
										                 <tr>
										                    <td colspan="3">No Reviews For this user</td>
										                 </tr>
										            <% } %> 								
						</div>	
						
						</div>
					</div>
					
					<div class="tab-pane" id="d">
						<div class="panel panel-default">
							<div class="panel-heading" align="middle" style="font-size: auto"ng-model="product_name_model">
								<strong><h3>Add Reviews</h3></strong>
							</div>
							<div class="panel-body" >
								<form action = '/saveUserReview/<%=host[0].user_id%>' method = 'post'>								
								<textarea rows="4" cols="150" name="review" id="review" placeholder="Provide review on user here...."></textarea><br><br>
								<div class="form-group">
                        			<div class="col-sm-4">
                    		        <select class="form-control" id="rating" name="rating">
                            		    <option>Select Rating</option>
						 			   	<option>Excellent</option>
									    <option>Good</option>
									    <option>Average</option>
						    			<option>Bad</option>
						    			<option>Very Bad</option>
                           	 		</select>
                        			</div>
									<div class="col-sm-6" align="right">
										<button  class="btn btn-danger" style="display: block;width:100%;font-size:auto" type="submit" ><strong>Submit Review & Rating</strong></button>
									</div>
								</div>
								</form>
							</div>
						</div>
					</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>