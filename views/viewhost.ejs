<style>
input[type=text]:focus {
	outline: none;
	box-shadow: 0px 1px 0px 0px #008489;
}

select:focus {
	outline: none;
	box-shadow: 0px 1px 0px 0px #008489;
}

.icon-facebook:before {
	content: '\f16e';
}

.icon:before {
	line-height: 1;
}

.icon:before {
	font-family: Airglyphs, sans-serif;
}

a:focus {
	outline: none
}

button:focus {
	outline: none
}

*:focus {
    outline: 0 !important;
}

.nav-pills>li.active>a, .nav-pills>li.active>a:hover, .nav-pills>li.active>a:focus
	{
	color: black;
	background-color: white;
	font-weight: 800;
}
</style>

